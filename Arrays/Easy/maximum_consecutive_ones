class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int current1 = 0;
        int max1 = 0;
        for (int i = 0; i < nums.length; i++) {
            if (nums[i] == 1) {
                current1 ++;
                max1 = Math.max(current1, max1);
            }
            else {
                current1 = 0;
            }
        }
        return max1;
    }
}